{
    "name":"LOU Extensions",
    "version":"0.0.1",
    "manifest_version":2,
    "description":"Collection of extensions for Lord of Ultima (http://www.lordofultima.com/)",
    "converted_from_user_script":true,
    "content_scripts":[
        {
            "exclude_globs":[  ],
            "exclude_matches":[  ],
            "include_globs":[ "http://prodgame*.lordofultima.com/*", "http://localhost*" ],
            "js":[ "chrome.user.js" ],
            "matches":[ "http://*/*", "https://*/*" ],
            "run_at":"document_idle"
        }
    ],
    "minimum_chrome_version":"18",
    "web_accessible_resources":[
        "manifest.json",
        "bos/greasemonkey.light.js",
        "bos/sprintf-0.7-beta1.js",
        "bos/Const.js",
        "bos/Localization.js",
        "bos/gui/ResourcesFillerWidget.js",
        "bos/BatchResourcesFiller.js",
        "bos/ResourcesFiller.js",
        "bos/Server.js",
        "bos/Storage.js",
        "bos/net/CommandManager.js",
        "bos/Global.js",
        "bos/Tweaks.js",
        "bos/SharestringConverter.js",
        "bos/Utils.js",
        "bos/CityTypes.js",
        "bos/City.js",
        "bos/Main.js",
    ]
}